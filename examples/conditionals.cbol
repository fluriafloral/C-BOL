// IF

IF (x < NOT y)
    x = y;
END-IF;

IF (x < 1)
    x = y;
END-IF;

IF ("Owweee" < 1 AND 1)
    x = y;
END-IF;

IF x < y
    x = y;
ELIF (x < 10)
    x = 30;
END-IF;

// SWITCH
