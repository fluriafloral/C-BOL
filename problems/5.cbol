PROCEDURE mdc(INTEGER n, INTEGER m, INTEGER @ r)
    IF (m % n == 0)
       @ r = n;
    ELIF (n % m == 0)
        @ r = m;
    ELIF (m > n)
        mdc(n, m % n, r);
    ELSE
        mdc(m, n % m, r);
    END-IF;
END-PROCEDURE;

PROGRAM
    INTEGER n, m;
    INTEGER @ r;

    INPUT n;
    INPUT m;

    CALL mdc(n, m, r);

    OUTPUT "O mdc Ã©: ";
    OUTPUT @ r;
END-PROGRAM;